      "level": "h1"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "The HTML <audio> element is used to play an audio file on a web page.",
      "html": "<p class=\"intro\">The HTML <code>&lt;audio&gt;</code> element is used to \nplay an audio file on a web page.</p>"
    },
