      "code_html": "<pre class=\"command-line language-shell\"><code class=\"language-shell\"><span class=\"command-line-prompt\"><span data-host=\"localhost\" data-user=\"user\"></span></span><span class=\"token function\">git</span> cherry-pick abc1234 -x</code></pre>",
      "code_classes": [
