    {
      "type": "header",
      "metadata": {
        "level": "h2"
      },
      "text": "Apply Without Committing",
      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Use --no-commit (or -n ) to apply the changes, but not create a commit yet. This lets you make more changes before committing:",
      "html": "<p>Use <code class=\"language-shell\">--no-commit</code> (or <code class=\"language-shell\">-n</code>) to apply the changes, but not create a commit yet. This lets you make more changes before committing:</p>"
    },
    {
      "type": "code",
      "metadata": {
        "language": "unknown",
        "tryItLink": null,
        "syntaxHighlighting": [],
        "classList": [
          "command-line",
          "language-shell"
        ],
        "isNotranslate": false
      },
      "code": "git\ncherry-pick abc1234 --no-commit",
      "language": "unknown",
      "code_html": "<pre class=\"command-line language-shell\"><code class=\"language-shell\"><span class=\"command-line-prompt\"><span data-host=\"localhost\" data-user=\"user\"></span></span><span class=\"token function\">git</span> cherry-pick abc1234 --no-commit</code></pre>",
      "code_classes": [
        "command-line",
        "language-shell"
