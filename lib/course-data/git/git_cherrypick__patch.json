    {
      "type": "code",
      "metadata": {
        "language": "unknown",
        "tryItLink": null,
        "syntaxHighlighting": [],
        "classList": [
          "command-line",
          "language-shell"
        ],
        "isNotranslate": false
      },
      "code": "git\ncherry-pick abc1234 -x",
