        "tryItLink": "https://www.w3schools.com/css/tryit.asp?filename=trycss_table_border",
        "syntaxHighlighting": [
          {
            "text": "\ntable, th, td {\n\u00a0\u00a0border: 1px solid;\n}\n ",
            "color": "rgb(153, 0, 85)",
            "class": "cssselectorcolor"
          },
          {
            "text": "{",
            "color": "rgb(153, 153, 153)",
            "class": "cssdelimitercolor"
          },
          {
            "text": "\n\u00a0\u00a0border: 1px solid;\n",
            "color": "rgb(215, 58, 73)",
            "class": "csspropertycolor"
          },
          {
            "text": ": 1px solid;",
            "color": "rgb(0, 92, 197)",
            "class": "csspropertyvaluecolor"
          },
          {
            "text": ":",
            "color": "rgb(153, 153, 153)",
            "class": "cssdelimitercolor"
          },
