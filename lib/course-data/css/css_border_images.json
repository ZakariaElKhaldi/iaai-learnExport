      "type": "text",
      "metadata": {},
      "text": "Example 1:",
      "html": "<p>Example 1:</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "border-image: url(border.png) 50 round;",
      "html": "<p id=\"border_image3\">border-image: url(border.png) 50 round;</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Example 2:",
      "html": "<p>Example 2:</p>"
    },
