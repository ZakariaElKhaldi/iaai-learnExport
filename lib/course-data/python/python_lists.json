      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Lists are used to store multiple items in a single variable.",
      "html": "<p>Lists are used to store multiple items in a single variable.</p>"
    },
    {
      "type": "text",
