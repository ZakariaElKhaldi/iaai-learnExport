      "metadata": {
        "language": "unknown",
        "tryItLink": "https://www.w3schools.com/php/phptryit.asp?filename=tryphp_variables_quotes",
        "syntaxHighlighting": [],
        "classList": [
          "notranslate",
          "w3-white",
          "language-php"
        ],
        "isNotranslate": true
      },
      "code": "$x\n\n=\n\n\"John\"\n;\n\n\necho\n\n$x\n;",
      "language": "php",
      "code_html": "<pre class=\"notranslate w3-white language-php\" tabindex=\"0\"><code class=\"language-php\"><span class=\"token variable\">$x</span> <span class=\"token operator\">=</span> <span class=\"token string double-quoted-string\">\"John\"</span><span class=\"token punctuation\">;</span>\n<span class=\"token keyword keyword-echo\">echo</span> <span class=\"token variable\">$x</span><span class=\"token punctuation\">;</span>\n</code></pre>",
