      "links": [
        {
          "text": "PHP Strings chapter",
          "href": "https://www.w3schools.com/php_string.asp",
          "title": ""
        }
      ],
      "text": "String variables can be declared either by using double or single quotes, but you should be aware of the differences. Learn more about the differences in the PHP Strings chapter .",
      "html": "<p>String variables can be declared either by using double or single quotes, but you should\nbe aware of the differences. Learn more about the differences in the <a href=\"php_string.asp\">PHP Strings chapter</a>.</p>"
    },
    {
      "type": "header",
      "metadata": {
        "level": "h2"
      },
      "text": "Assign Multiple Values",
      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "You can assign the same value to multiple variables in one line:",
      "html": "<p>You can assign the same value to multiple variables in one line:</p>"
    },
    {
      "type": "code",
      "metadata": {
        "language": "unknown",
        "tryItLink": "https://www.w3schools.com/php/phptryit.asp?filename=tryphp_variables_multiple",
        "syntaxHighlighting": [],
        "classList": [
          "notranslate",
          "w3-white",
          "language-php"
        ],
        "isNotranslate": true
      },
      "code": "$x\n\n=\n\n$y\n\n=\n\n$z\n\n=\n\n\"Fruit\"\n;",
      "language": "php",
      "code_html": "<pre class=\"notranslate w3-white language-php\" tabindex=\"0\"><code class=\"language-php\"><span class=\"token variable\">$x</span> <span class=\"token operator\">=</span> <span class=\"token variable\">$y</span> <span class=\"token operator\">=</span> <span class=\"token variable\">$z</span> <span class=\"token operator\">=</span> <span class=\"token string double-quoted-string\">\"Fruit\"</span><span class=\"token punctuation\">;</span>\n</code></pre>",
      "code_classes": [
        "notranslate",
        "w3-white",
        "language-php"
