      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "PHP has two operators that are specially designed for strings.",
      "html": "<p>PHP has two operators that are specially designed for strings.</p>"
    },
    {
      "type": "code",
      "metadata": {
        "language": "unknown",
        "tryItLink": "https://www.w3schools.com/php/phptryit.asp?filename=tryphp_oper_string1",
