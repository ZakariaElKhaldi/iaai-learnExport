    {
      "type": "header",
      "metadata": {
        "level": "h2"
      },
      "text": "Double Quotes",
      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Text must be wrapped inside double quotations marks \"\" .",
      "html": "<p>Text must be wrapped inside double quotations marks <code>\"\"</code>.</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "If you forget the double quotes, an error occurs:",
      "html": "<p>If you forget the double quotes, an error occurs:</p>"
    },
    {
      "type": "code",
      "metadata": {
        "language": "unknown",
        "tryItLink": "https://www.w3schools.com/java/tryjava.asp?filename=demo_output_quotes_err",
        "syntaxHighlighting": [],
        "classList": [
          "w3-white",
          "language-java"
        ],
        "isNotranslate": false
      },
      "code": "System\n.\nout\n.\nprintln\n(\n\"This sentence will work!\"\n)\n;",
      "language": "java",
      "code_html": "<pre class=\"w3-white language-java\" tabindex=\"0\"><code class=\"language-java\"><span class=\"token class-name\">System</span><span class=\"token punctuation\">.</span>out<span class=\"token punctuation\">.</span><span class=\"token function\">println</span><span class=\"token punctuation\">(</span><span class=\"token string\">\"This sentence will work!\"</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span></code></pre>",
      "code_classes": [
        "w3-white",
        "language-java"
      ],
      "tryItLink": "https://www.w3schools.com/java/tryjava.asp?filename=demo_output_quotes_err",
