    {
      "type": "text",
      "metadata": {},
      "text": "Notes can be used to notify the user about something special: danger, success, information or warning",
      "html": "<p>Notes can be used to notify the user about something special: danger, success, information or warning</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Danger! Some text...",
      "html": "<p><strong>Danger!</strong> Some text...</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Success! Some text...",
      "html": "<p><strong>Success!</strong> Some text...</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Info! Some text...",
      "html": "<p><strong>Info!</strong> Some text...</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "Warning! Some text...",
      "html": "<p><strong>Warning!</strong> Some text...</p>"
