      "level": "h2"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "The DEFAULT constraint provides a default value for a column.",
      "html": "<p>The <code>DEFAULT</code> constraint provides a default \nvalue for a column.</p>"
    },
    {
      "type": "text",
      "metadata": {},
      "text": "The default value will be added to all new records if no other value is specified.",
      "html": "<p>The default value will be added to all new records if no other value is specified.</p>"
    },
