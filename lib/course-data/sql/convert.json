      "code": "CONVERT()",
      "language": "html",
      "code_html": "<div class=\"w3-code w3-border notranslate\">\nCONVERT(<i>data_type(length)</i><em>,</em><i> expression</i><em>,</em><i> style</i>)</div>",
      "code_classes": [
        "w3-code",
